{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ES2021",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": false,
    "noEmit": false,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false,
    "allowImportingTsExtensions": false,
  }
}